@model IEnumerable<WebApplication2.Models.TBLUYE>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>ÜYELER</h2>

<p>
    @Html.ActionLink("Üye Oluştur", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TBLYETKI.YETKIAD)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AD)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SOYAD)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NUMARA)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EMAIL)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SIFRE)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TBLYETKI.YETKIAD)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AD)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SOYAD)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NUMARA)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EMAIL)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SIFRE)
        </td>
        <td>
            @Html.ActionLink("DÜZENLE", "Edit", new { id = item.UYEID }) |
            @Html.ActionLink("DETAY", "Details", new { id=item.UYEID }) |
            @Html.ActionLink("SİL", "Delete", new { id=item.UYEID })
        </td>
    </tr>
}

</table>
